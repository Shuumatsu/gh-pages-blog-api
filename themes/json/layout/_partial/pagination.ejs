<%
const filters = ['title', 'date', 'updated', 'comments', 'excerpt', 'source', 'path', 'permalink', 'link', 'published', 'categories', 'tags']
const datas = JSON.stringify(base.map(post => {
  const data = {}
  for (let filter of filters) data[filter] = post[filter]
  data.tags = data.tags.data.map(document => document.name)
  data.categories = data.categories.data.map(document => document.name)
  return data
}))
-%>
<%- datas %>
